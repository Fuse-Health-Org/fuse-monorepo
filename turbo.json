{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "bundle/**",
        ".next/**"
      ]
    },
    "build:windows": {
      "dependsOn": [
        "^build:windows"
      ],
      "outputs": [
        "dist/**",
        "bundle/**",
        ".next/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^dev"
      ],
      "env": [

      ]
    },
    "@fuse/frontend#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@fuse/api#dev"]
    },
    "@fuse/admin-frontend#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@fuse/api#dev"]
    },
    "@fuse/tenant-portal-frontend#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@fuse/api#dev"]
    },
    "@fuse/doctor-portal-frontend#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@fuse/api#dev"]
    },
    "@fuse/affiliate-portal-frontend#dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@fuse/api#dev"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "pm2:stop": {
      "cache": false
    },
    "pm2:reload": {
      "cache": false,
      "dependsOn": [
        "build",
        "pm2:stop"
      ]
    }
  },
  "globalDependencies": [
    ".env.local",
    ".env.production"
  ],
  "globalEnv": [
  ]
}