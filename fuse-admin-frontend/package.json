{
  "name": "@fuse/admin-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wait-on tcp:localhost:3001 && pnpm with-env next dev -p 3002",
    "build": "NODE_ENV=production pnpm with-env next build",
    "build:windows": "cross-env NODE_ENV=production pnpm with-env next build",
    "start": "next start -p 3020",
    "lint": "next lint",
    "pm2:start": "pnpm with-env pm2 start npm --name admin-frontend --node-args='--max-old-space-size=1024 --no-warnings' -- start",
    "pm2:reload": "pnpm build && pnpm with-env pm2 reload admin-frontend --update-env",
    "pm2:reload-no-build": "pnpm with-env pm2 reload admin-frontend --update-env",
    "pm2:stop": "pm2 stop admin-frontend",
    "clean": "git clean -xdf .cache .turbo dist .next node_modules",
    "with-env": "dotenv -e ../.env.local --",
    "test:e2e": "pnpm with-env playwright test",
    "test:e2e:ui": "pnpm with-env playwright test --ui"
  },
  "dependencies": {
